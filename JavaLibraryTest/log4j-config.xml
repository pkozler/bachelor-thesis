<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Properties>
        <Property name="format">%method%n%msg%n</Property>
        <Property name="PrintStreamFilename">PrintStreamTest.txt</Property>
        <Property name="StringFilename">StringTest.txt</Property>
        <Property name="StringBuilderFilename">StringBuilderTest.txt</Property>
        <Property name="ByteFilename">ByteTest.txt</Property>
        <Property name="ShortFilename">ShortTest.txt</Property>
        <Property name="IntegerFilename">IntegerTest.txt</Property>
        <Property name="LongFilename">LongTest.txt</Property>
        <Property name="FloatFilename">FloatTest.txt</Property>
        <Property name="DoubleFilename">DoubleTest.txt</Property>
        <Property name="BooleanFilename">BooleanTest.txt</Property>
        <Property name="CharacterFilename">CharacterTest.txt</Property>
        <Property name="MathFilename">MathTest.txt</Property>
        <Property name="ArraysFilename">ArraysTest.txt</Property>
        <Property name="ArrayListFilename">ArrayListTest.txt</Property>
        <Property name="LinkedListFilename">LinkedListTest.txt</Property>
        <Property name="CollectionsFilename">CollectionsTest.txt</Property>
        <Property name="StringTokenizerFilename">StringTokenizerTest.txt</Property>
        <Property name="ScannerFilename">ScannerTest.txt</Property>
    </Properties>
    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout pattern="${format}"/>
        </Console>
        <File name="PrintStreamFile" fileName="logs/${PrintStreamFilename}">
            <PatternLayout pattern="${format}"/>
        </File>
        <File name="StringFile" fileName="logs/${StringFilename}">
            <PatternLayout pattern="${format}"/>
        </File>
        <File name="StringBuilderFile" fileName="logs/${StringBuilderFilename}">
            <PatternLayout pattern="${format}"/>
        </File>
        <File name="ByteFile" fileName="logs/${ByteFilename}">
            <PatternLayout pattern="${format}"/>
        </File>
        <File name="ShortFile" fileName="logs/${ShortFilename}">
            <PatternLayout pattern="${format}"/>
        </File>
        <File name="IntegerFile" fileName="logs/${IntegerFilename}">
            <PatternLayout pattern="${format}"/>
        </File>
        <File name="LongFile" fileName="logs/${LongFilename}">
            <PatternLayout pattern="${format}"/>
        </File>
        <File name="FloatFile" fileName="logs/${FloatFilename}">
            <PatternLayout pattern="${format}"/>
        </File>
        <File name="DoubleFile" fileName="logs/${DoubleFilename}">
            <PatternLayout pattern="${format}"/>
        </File>
        <File name="BooleanFile" fileName="logs/${BooleanFilename}">
            <PatternLayout pattern="${format}"/>
        </File>
        <File name="CharacterFile" fileName="logs/${CharacterFilename}">
            <PatternLayout pattern="${format}"/>
        </File>
        <File name="MathFile" fileName="logs/${MathFilename}">
            <PatternLayout pattern="${format}"/>
        </File>
        <File name="ArraysFile" fileName="logs/${ArraysFilename}">
            <PatternLayout pattern="${format}"/>
        </File>
        <File name="ArrayListFile" fileName="logs/${ArrayListFilename}">
            <PatternLayout pattern="${format}"/>
        </File>
        <File name="LinkedListFile" fileName="logs/${LinkedListFilename}">
            <PatternLayout pattern="${format}"/>
        </File>
        <File name="CollectionsFile" fileName="logs/${CollectionsFilename}">
            <PatternLayout pattern="${format}"/>
        </File>
        <File name="StringTokenizerFile" fileName="logs/${StringTokenizerFilename}">
            <PatternLayout pattern="${format}"/>
        </File>
        <File name="ScannerFile" fileName="logs/${ScannerFilename}">
            <PatternLayout pattern="${format}"/>
        </File>

    </Appenders>
    <Loggers>
        <Root level="ALL">
            <AppenderRef ref="console"></AppenderRef>
        </Root>
        <Logger name="test.Hlavni" level="INFO">
            <AppenderRef ref="soubor1"></AppenderRef>
        </Logger>
        <Logger name="test.PrintStreamTest" level="INFO">
            <AppenderRef ref="PrintStreamFile"></AppenderRef>
        </Logger>
        <Logger name="test.StringTest" level="INFO">
            <AppenderRef ref="StringFile"></AppenderRef>
        </Logger>
        <Logger name="test.StringBuilderTest" level="INFO">
            <AppenderRef ref="StringBuilderFile"></AppenderRef>
        </Logger>
        <Logger name="test.ByteTest" level="INFO">
            <AppenderRef ref="ByteFile"></AppenderRef>
        </Logger>
        <Logger name="test.ShortTest" level="INFO">
            <AppenderRef ref="ShortFile"></AppenderRef>
        </Logger>
        <Logger name="test.IntegerTest" level="INFO">
            <AppenderRef ref="IntegerFile"></AppenderRef>
        </Logger>
        <Logger name="test.LongTest" level="INFO">
            <AppenderRef ref="LongFile"></AppenderRef>
        </Logger>
        <Logger name="test.FloatTest" level="INFO">
            <AppenderRef ref="FloatFile"></AppenderRef>
        </Logger>
        <Logger name="test.DoubleTest" level="INFO">
            <AppenderRef ref="DoubleFile"></AppenderRef>
        </Logger>
        <Logger name="test.BooleanTest" level="INFO">
            <AppenderRef ref="BooleanFile"></AppenderRef>
        </Logger>
        <Logger name="test.CharacterTest" level="INFO">
            <AppenderRef ref="CharacterFile"></AppenderRef>
        </Logger>
        <Logger name="test.MathTest" level="INFO">
            <AppenderRef ref="MathFile"></AppenderRef>
        </Logger>
        <Logger name="test.ArraysTest" level="INFO">
            <AppenderRef ref="ArraysFile"></AppenderRef>
        </Logger>
        <Logger name="test.ArrayListTest" level="INFO">
            <AppenderRef ref="ArrayListFile"></AppenderRef>
        </Logger>
        <Logger name="test.LinkedListTest" level="INFO">
            <AppenderRef ref="LinkedListFile"></AppenderRef>
        </Logger>
        <Logger name="test.CollectionsTest" level="INFO">
            <AppenderRef ref="CollectionsFile"></AppenderRef>
        </Logger>
        <Logger name="test.StringTokenizerTest" level="INFO">
            <AppenderRef ref="StringTokenizerFile"></AppenderRef>
        </Logger>
        <Logger name="test.ScannerTest" level="INFO">
            <AppenderRef ref="ScannerFile"></AppenderRef>
        </Logger>
    </Loggers>
</Configuration>